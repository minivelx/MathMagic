<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.movil.p.mathmagic">

    <application
        android:allowBackup="true"
        android:icon="@drawable/huevo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LogActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".iniciar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.movil.p.mathmagic.LogActivity" />
        </activity>
        <activity android:name=".GetExamQuest" />
        <activity android:name=".MainNivel" />
        <activity android:name=".MainEjercicios"></activity>
